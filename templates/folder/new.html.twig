{# templates/folder/new.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
    <h1>Create new Folder</h1>

    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.parent) }}
